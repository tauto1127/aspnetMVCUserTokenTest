@model webUserLoginTest.Models.ViewModel.UserIndexViewModel
@{
    ViewBag.Title = "title";
    Layout = "_Layout";
    
}

<h2>title</h2>
<table>
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Users[0].Id)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.Users[0].Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users[0].CreatedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users[0].PasswordHash)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users[0].PasswordSalt)
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Users)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(model => item.Name)
            </td>
            <td>
                @Html.DisplayFor(model => item.CreatedDate)
            </td>
            <td>
                @Html.DisplayFor(model => item.PasswordHash)
            </td>
            <td>
                @Html.DisplayFor(model => item.PasswordSalt)
            </td>
        </tr>
    }
    </tbody>
</table>
